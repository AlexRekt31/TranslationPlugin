<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <translation.documentationElementProvider
                language="go"
                implementationClass="cn.yiiguxing.plugin.translate.provider.GoDocumentationElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                id="Translation.TranslatedDocumentationProvider.Go"
                order="first"
                language="go"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatedDocumentationProvider"/>
    </extensions>
</idea-plugin>