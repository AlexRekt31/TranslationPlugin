<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <docElementProvider
                language="Python"
                implementationClass="cn.yiiguxing.plugin.translate.provider.PythonDocumentationElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                id="Translation.TranslatedDocumentationProvider"
                order="first"
                language="Python"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatedDocumentationProvider"/>
    </extensions>
</idea-plugin>