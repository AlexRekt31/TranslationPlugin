<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <ignoredDocumentationElementsProvider
                language="PHP"
                implementationClass="cn.yiiguxing.plugin.translate.provider.PhpIgnoredDocumentationElementsProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                id="Translation.TranslatedDocumentationProvider"
                order="first"
                language="PHP"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatedDocumentationProvider"/>
    </extensions>
</idea-plugin>